---
import FrameworkSwitcher from "@/components/FrameworkSwitcher";
import TOC from "@/components/TOC";
import Default from "@astrojs/starlight/components/PageSidebar.astro";
import type { Props } from "@astrojs/starlight/props";

const isAjToc = Astro.props.entry.data.ajToc != undefined;
const frameworks = Astro.props.entry.data.frameworks;
---

<FrameworkSwitcher client:load frameworks={frameworks} />

{
  isAjToc ? (
<TOC client:load astro={Astro.props} />
  ) : (
<Default {...Astro.props}>
  <slot />
</Default>
  )
}